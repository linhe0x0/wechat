{
  "name": "@sqrtthree/wechat",
  "version": "0.2.1",
  "description": "Wechat SDK",
  "main": "dist/index.js",
  "types": "dist/types",
  "scripts": {
    "dev": "rimraf dist && tsc --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "prebuild": "npm run lint",
    "build": "rimraf dist && tsc",
    "type-check": "tsc --noEmit",
    "lint": "npm run type-check && eslint --ext ts ./src",
    "lint-with-fix": "eslint --ext ts --fix ./src",
    "test": "jest --coverage --onlyChanged",
    "prepublishOnly": "npm run build"
  },
  "author": "sqrtthree <imsqrtthree@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "npm run lint"
  },
  "dependencies": {
    "consola": "~2.15.0",
    "got": "~11.6.2",
    "lodash": "~4.17.20"
  },
  "devDependencies": {
    "@sqrtthree/prettier-config": "~2.0.1",
    "@sqrtthree/tsconfig": "~0.1.0",
    "@types/lodash": "~4.14.161",
    "@types/node": "~14.10.2",
    "@typescript-eslint/eslint-plugin": "~4.1.1",
    "@typescript-eslint/parser": "~4.1.1",
    "eslint": "~7.9.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-prettier": "~3.1.4",
    "husky": "~4.3.0",
    "lint-staged": "~10.3.0",
    "prettier": "~2.1.2",
    "rimraf": "~3.0.2",
    "typescript": "~4.0.2"
  }
}
